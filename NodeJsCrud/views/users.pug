h1 List of users
input(type='submit', value='Add new user')
table
  thead
    tr 
      th Name
      th Last Name
      th Email
      th Age
      th Edit
      th Delete
  tbody
    each user in users 
      tr
        td= user.userName
        td= user.userSurname
        td= user.email
        td= user.age
        td
          input(id= user.id, type= 'submit' value= 'Edit')
        td 
          input(id= user.id, type= 'submit' value= 'Delete')